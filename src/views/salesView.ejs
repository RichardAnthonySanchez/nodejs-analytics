<!DOCTYPE html>
<html>
  <head>
    <title>Sales Agent Data</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        margin: 2rem;
      }
      h1 {
        margin-bottom: 1rem;
      }
      table {
        width: 100%;
        border-collapse: collapse;
      }
      th,
      td {
        padding: 0.75rem;
        border: 1px solid #ccc;
        text-align: left;
      }
      th {
        background: #f2f2f2;
      }
      tr:nth-child(even) {
        background-color: #fafafa;
      }
    </style>
  </head>
  <body>
    <h1>Sales Agent Data</h1>
    <table>
      <thead>
        <tr>
          <th>Agent</th>
          <th>Job Number</th>
          <th>Profit ($)</th>
          <th>Status</th>
          <th>Lead Source</th>
          <th>Date</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((record) => { %>
        <tr>
          <td><%= record.agent %></td>
          <td><%= record.jobNumber %></td>
          <td><%= record.profit.toLocaleString() %></td>
          <td><%= record.status %></td>
          <td><%= record.lead %></td>
          <td><%= new Date(record.timestamp).toLocaleDateString() %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
