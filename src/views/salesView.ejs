<!DOCTYPE html>
<html lang="en" data-theme="light">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Sales Agent Data</title>
    <link rel="stylesheet" href="/styles/output.css" />
  </head>
  <body class="bg-base-200 min-h-screen flex justify-center">
    <div
      class="flex flex-col w-full px-4 sm:px-6 md:px-12 lg:px-24 xl:px-32 gap-8"
    >
      <!-- Top Row -->
      <div
        class="flex flex-col md:flex-col lg:flex-row lg:justify-between gap-6"
      >
        <%- include('./partials/totalProfits', { totalProfit }) %> <%-
        include('./partials/rolesProfits', { roleProfits }) %>
      </div>

      <!-- Bottom Row -->
      <div
        class="flex flex-col md:flex-col lg:flex-row lg:justify-between gap-6"
      >
        <%- include('./partials/agentPerformance', { agentPerformance }) %> <%-
        include('./partials/leadProfits', { leadProfit }) %>
      </div>
    </div>

    <script>
      const urlParams = new URLSearchParams(window.location.search);
      const token = urlParams.get("token");
    </script>
  </body>
</html>
