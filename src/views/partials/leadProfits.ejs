<div class="card w-full max-w-sm bg-base-100 shadow-xl">
  <div class="card-body">
    <h2 class="card-title">Profit by Lead Source</h2>
    <ul class="space-y-2">
      <% leadProfit.forEach(item => { %>
      <li class="flex justify-between items-center">
        <span class="font-semibold"><%= item.lead %></span>
        <span>$<%= item.totalProfit.toLocaleString() %></span>
      </li>
      <% }) %>
    </ul>
  </div>
</div>
