<div class="card w-full max-w-sm bg-base-100 shadow-xl">
  <div class="card-body">
    <h2 class="card-title">Profit By Role</h2>
    <ul class="space-y-2">
      <% roleProfits.forEach(rp => { %>
      <li class="flex justify-between items-center">
        <span class="font-semibold"><%= rp.role %></span>
        <span>$<%= rp.totalProfit.toFixed(2) %></span>
      </li>
      <% }) %>
    </ul>
  </div>
</div>
